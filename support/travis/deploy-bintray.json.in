        "vcs_tag": "@CABL_VERSION_STRING@",
        "gpgSign": false
    },

    "files":
        [
        {"includePattern": "build/deploy/(.*)([.]zip)", "uploadPattern": "$1_@CABL_VERSION_STRING@$2"}
