{
    "package": {
        "name": "@CABL_DEPLOY_TARGET@",
        "repo": "cabl",
        "subject": "shaduzlabs",
        "desc": "Controller ABstraction Layer",
        "website_url": "www.shaduzlabs.com",
        "issue_tracker_url": "https://github.com/shaduzlabs/cabl/issues",
        "vcs_url": "https://github.com/shaduzlabs/cabl.git",
        "github_use_tag_release_notes": false,
        "licenses": ["MIT"],
        "labels": ["c++", "libusb", "cabl"],
        "public_download_numbers": false,
        "public_stats": false
    },

    "version": {
